{
  "$schema": "node_modules/albatros/schema.json",
  "name": "ru.topomatic.dwg",
  "displayName": "DWG reader",
  "description": "Плагин для загрузки файлов в формате dwg",
  "version": "1.0.6",
  "main": "src/index.ts",
  "scripts": {
    "serve": "albatros-cli serve",
    "build": "albatros-cli build"
  },
  "keywords": [
    "category::import"
  ],
  "author": {
    "name": "RoburFan",
    "url": "https://t.me/RoburFan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BorisMoniac/DWG-Reader.git"
  },
  "icon": "favicon",
  "license": "MIT",
  "devDependencies": {
    "albatros": "^1.2.3"
  },
  "albatros": {
    "importers": [
      {
        "target": "wdx",
        "description": "DWG",
        "cmd": "dwg",
        "filenamePattern": ".dwg"
      }
    ]
  },
  "dependencies": {
    "@mlightcad/libredwg-web": "^0.4.5"
  }
}
